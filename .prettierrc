    "singleQuote": true,
    "parser": "typescript",
    "tabWidth": 4,
    "bracketSpacing": true,
    "bracketSameLine": false,
    "printWidth": 120,
    "endOfLine": "lf",
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "overrides": [{
        "files": "*.json",
        "options": {
            "parser": "json"
        }
    }]
}
